<% if (session[:uid] == (LDAP_CONFIG['guest_user'])) %>
  <% cache("version_count_guest_#{@request_group.id}", :expires_in => 10.hours) do %>
    <%= formatted_int(@request_group.version_count) %>
  <% end %>
<% else %>
  <% cache("version_count_#{@request_group.id}", :expires_in => 10.minutes) do %>
    <%= formatted_int(@request_group.version_count) %>
  <% end %>
<% end %>