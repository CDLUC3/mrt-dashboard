{
  title: 'Audit Replic Status for <%= @object.ark %>'
  timeframe: '<%= @datestr %>',
  counts: [
    <% @count_by_status.each do |r| %>
      {
        title: '<%= r[0] %>',
        total: <%= r[1] %>,
        completed: <%= r[2] %>,
        started: <%= r[3].nil? ? 'null' : r[3] %>,
        error: <%= r[4].nil? ? 'null' : r[4] %>
      }
    <% end %>
  ]
}