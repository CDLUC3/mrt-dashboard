{
  "title": "Audit Replic Status for <%= @object.ark %>"
  "timeframe": "<%= @datestr %>",
  "counts": [
    <% @count_by_status.each_with_index do |i, r| %>
      <% if i > 0 %>,<% end %>
      {
        "title": "<%= r[0] %>",
        "total": <%= r[1] %>,
        "completed": <%= r[2] %>,
        "started": <%= r[3].nil? ? 'null' : r[3] %>,
        "error": <%= r[4].nil? ? 'null' : r[4] %>
      }
    <% end %>
  ]
}