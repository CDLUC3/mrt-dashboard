<div class="container_16">
  <% if params.has_key?('raw') then %>
    <%= render :partial=>'raw', :locals => {:info => @info } %>
  <% else %>
    <% case @info[RDF.type]
       when [Mrt::ORE['Aggregation']] %>
         <%= render :partial=>'object', :locals => {:info => @info } %>
    <% when [Mrt::File['File']] %>
         <%= render :partial=>'storage_file', :object => @info %>
    <% else %>
         <%= render :partial=>'raw', :locals => {:info => @info } %>
    <% end %>
  <% end %>
</div>
